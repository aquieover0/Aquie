{
    "compilerOptions": {
        "lib": ["ESNext"],
        "module": "CommonJS",
        "moduleResolution": "Node",
        "target": "ESNext",
        "sourceMap": false,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true,
        "resolveJsonModule": true,
        "importHelpers": true
    }
}